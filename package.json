{
  "name": "simple-note-chat",
  "version": "1.2.6",
  "description": "A simple plugin for chatting with LLMs in Obsidian.",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "version": "node version-bump.mjs && git add manifest.json versions.json",
    "test": "cd tests && npm test",
    "test:setup": "cd tests && npm install",
    "test:commands": "cd tests && npm run test:commands",
    "test:encryption": "cd tests && npm run test:encryption",
    "test:all": "npm run build && cd tests && npm test",
    "test:headless": "cd tests && npm test -- --headless"
  },
  "keywords": [
    "obsidian",
    "plugin",
    "chat",
    "ai",
    "llm"
  ],
  "author": "Aaron Scott",
  "license": "AGPL-3.0",
  "devDependencies": {
    "@types/node": "^16.11.6",
    "builtin-modules": "3.3.0",
    "esbuild": "0.25.0",
    "obsidian": "latest",
    "tslib": "2.4.0",
    "typescript": "4.7.4",
    "fs-extra": "^11.0.0"
  }
}
